{
  "printone": {
    "prefix": "printone",
    "body": [
      "println!(\"{}\", ${1});"
    ],
    "description": "Print one thing"
  },
  "struct": {
    "prefix": "tys",
    "body": [
      "#[derive(Debug)]",
      "struct ${1:Name} {",
      "    ${2:field}: ${3:Type}",
      "}"
    ],
    "description": "Struct + Debug"
  },
  "tafn": {
    "prefix": "tafn",
    "body": [
      "#[tokio::test]",
      "async fn ${1:name}() {",
      "    ${2:todo!();}",
      "}"
    ],
    "description": "Tokio test"
  }
}
