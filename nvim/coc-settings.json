{
  "codeLens.enable": true,
  "snippets.userSnippetsDirectory": "~/.config/nvim/snippets",
  "flutter.lsp.initialization.closingLabels": true,
  "flutter.lsp.initialization.onlyAnalyzeProjectsWithOpenFiles": false,
  "flutter.commands.devicesTimeout": 3,
  "flutter.UIPath": false,
  "flutter.sdk.searchPaths": [
      "/home/clive/snap/flutter/common/flutter"
  ],
  "flutter.autoOpenDevLog": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "dart",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "scala",
    "go"
  ],
  "prettier.disableSuccessMessage": true,
  "python.jediEnabled": false,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": false
      }
    }
  }
}
